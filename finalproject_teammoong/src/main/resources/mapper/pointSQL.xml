<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="point">
	<insert id="insertPoint" parameterType="_int">
		insert into point_tbl values
		(point_tbl_seq.nextval, #{memberNo}, 50)
	</insert>
	<insert id="insertFirstPoint" parameterType="_int">
		insert into point_tbl values
		(point_tbl_seq.nextval, #{memberNo}, 2000)
	</insert>
	<select id="selectAllPoint" parameterType="_int" resultType="point">
		select
			point_ea as pointEa
		from point_tbl where member_no = #{memberNo}
	</select>
</mapper>
