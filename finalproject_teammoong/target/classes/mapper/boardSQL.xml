<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="board">
	<insert id="insertBoard" parameterType="b">
		insert into board values(board_seq.nextval,#{boardWriter},#{boardName},#{categoryNo},#{boardContent},#{boardPrice},#{boardDate},#{accountWriter},#{accountName},#{accountBank},#{boardStart},#{boardEnd},#{boardDeatail},#{refernceLink},#{deliveryDate},#{boardEtc},#{boardStatus},#{boardCount},to_char(sysdate,'yyy-mm-dd'))
	</insert>
</mapper>
